  {
    "name": "Anti Distraction Tool",
    "description": "An extension to keep you out of social media during when you want to get things done.",
    "version": "2.0",
    "permissions": [
      "activeTab",
      "tabs",
      "identity"
    ],
    "background": {
      "scripts": [
        "background.js",
        "jsrsasign-all-min.js",
        "kuchika.js"
      ],
      "persistent": false
    },
    "browser_action": {
      "default_title": "Get current active tab"
    },
    "manifest_version": 2
  }
  